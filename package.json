{
    "name": "bullet-wasm",
    "version": "0.0.0",
    "description": "bullet physics via rust wasm",
    "scripts": {
        "start": "cross-env RUSTFLAGS=\"-Ctarget-feature=+simd128,+atomics,+bulk-memory,+mutable-globals\" webpack serve --mode development",
        "build": "cross-env RUSTFLAGS=\"-Ctarget-feature=+simd128,+atomics,+bulk-memory,+mutable-globals\" webpack --mode production",
        "ubuntu-setup": "sudo apt-get install -y rustc cargo clang llvm",
        "vercel-build": "npm run ubuntu-setup && npm run build",
        "lint": "eslint --ext .ts,.tsx . --fix --cache"
    },
    "author": "noname0310",
    "license": "MIT",
    "devDependencies": {
        "@babylonjs/core": "^7.10.0",
        "@babylonjs/inspector": "^7.10.0",
        "@babylonjs/loaders": "^7.10.0",
        "@babylonjs/materials": "^7.10.0",
        "@typescript-eslint/eslint-plugin": "^7.12.0",
        "@typescript-eslint/parser": "^7.12.0",
        "@wasm-tool/wasm-pack-plugin": "^1.7.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-simple-import-sort": "^12.1.0",
        "eslint-webpack-plugin": "^4.2.0",
        "file-loader": "^6.2.0",
        "html-loader": "^5.0.0",
        "html-webpack-plugin": "^5.6.0",
        "rimraf": "^5.0.7",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-glslminify-transformer": "^0.0.2",
        "typescript-transform-paths": "^3.4.7",
        "url": "^0.11.3",
        "wasm-pack": "^0.12.1",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    },
    "sideEffects": false
}
